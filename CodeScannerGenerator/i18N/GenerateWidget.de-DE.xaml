<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib">

    <sys:String x:Key="ResourceDictionaryName">Loc-en-US</sys:String>

    <!--Generate card-->
    <sys:String x:Key="Type">Typ:</sys:String>
    <sys:String x:Key="Width">Breite:</sys:String>
    <sys:String x:Key="Height">Höhe:</sys:String>
    <sys:String x:Key="Text">Text:</sys:String>
    <sys:String x:Key="Save">Speichern</sys:String>

    <!--Popups-->
    <sys:String x:Key="EmptyTextTitle">Kein Text zum Erzeugen des Codes</sys:String>
    <sys:String x:Key="EmptyTextMessage">Es ist kein Text vorhanden, aus dem ein Code generiert werden kann. Bitte geben Sie etwas ein und versuchen.</sys:String>
    <sys:String x:Key="NoGeneratedTitle">Kein Code generiert</sys:String>
    <sys:String x:Key="NoGeneratedMessage">Es ist kein Code generiert. Bitte generieren Sie ihn zuerst.</sys:String>
</ResourceDictionary>